{
  "version": "1.0.0",
  "name": "webpack-angular",
  "base": "/demo",
  "index": "index.html",
  "main": "app.js",
  "server": {
    "host": "http://localhost:8009"
  },
  "output": {
    "app": "app"
  },
  "profile": {
    "dll": "demo"
  },
  "vendors": ["angular", "@damo/l20n/dist/ng/1.0.0/l20n"],
  "devtool": "#cheap-source-map",
  "locals": {
    "available": ["en-US", "zh-CN"],
    "default": "zh-CN",
    "form": "properties",
    "to": "json"
  }
}